{
  "name": "@mindbox/utils",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build:swc -w",
    "build": "rm -rf dist && pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"**/*.{ts,js}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "^0.5.11"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.6.7",
    "eslint": "^8.57.0",
    "eslint-config-acme": "workspace:*",
    "typescript": "^5.5.3"
  }
}